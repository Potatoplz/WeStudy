<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="CalendarMapper">

	<insert id="insert" parameterType="kr.co.westudy.mypage.CalendarDTO" useGeneratedKeys="true" keyProperty="cal_id">
    INSERT INTO calendar ( cal_ title, cal_start, cal_end )
    VALUES (  #{title}, #{start}, #{end} )
    </insert>

</mapper>
